@model IList<ConstructorListModel>
@{
    ViewBag.Title = "安拆人员列表";
}
<h2>
    安拆人员列表</h2>
@Html.ActionLink("注册新安拆人员", "Add")
<table width="100%">
    <tr>
        <th>
            操作
        </th>
        <th>
            备案编号
        </th>
        <th>
            姓名
        </th>
        <th>
            所在单位
        </th>
        <th>
            类别
        </th>
        <th>
            上岗证号
        </th>
        <th>
            上保情况
        </th>
        <th>
            正在操机
        </th>
        <th>
            输入日期
        </th>
        <th>
            打印
        </th>
        <th>
            备注
        </th>
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("修改", "Edit", new { id = m.Id }, new { @class = "lnkEdit" })
                @Html.ActionLink("删除", "Delete", new { id = m.Id }, new { @class = "lnkDel" })
                @Html.ActionLink("打印", "Print", new { id = m.Id }, new { @class = "lnkPrint" })
            </td>
            <td>@m.NO
            </td>
            <td>
                @m.Name
            </td>
            <td>@m.Employer
            </td>
            <td>@m.Category
            </td>
            <td>@m.CertificationNO
            </td>
            <td>@m.Insurance
            </td>
            <td>@m.BirthYear
            </td>
            <td>@m.RegisterDate.ToString("yyyy-MM-dd")
            </td>
            <td>@Html.Raw((m.IsPrinted) ? "是" : "")
            </td>
            <td>@m.Comment
            </td>
        </tr>
    }
</table>
